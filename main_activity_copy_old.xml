<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/lay1"
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        android:padding="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/besoin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:height="200dp"
            android:background="@drawable/rounded_corner"
            android:fontFamily="@font/amaranth"
            android:gravity="center_horizontal"
            android:minHeight="48dp"
            android:paddingStart="5dp"
            android:paddingTop="5dp"
            android:text="@string/besoin_n_d_un_service_n_intellectuel"
            android:textColor="@color/white"
            android:textSize="35sp"
            tools:ignore="RtlSymmetry"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <TextView
            android:id="@+id/soft"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-18dp"
            android:height="150dp"
            android:background="@drawable/softxl150"
            android:minHeight="48dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TableLayout
            android:id="@+id/tlay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-2dp"
            android:paddingBottom="40dp"
            android:stretchColumns="1">

            <TableRow
                android:id="@+id/newRow"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/BlueOf2cs">

                <TextView
                    android:id="@+id/newCell1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/BlueOf2cs"
                    android:fontFamily="@font/actor"
                    android:padding="8dp"
                    android:text="@string/nos_services_sont_les_suivants"
                    android:textColor="@color/bleu_claire"
                    android:textSize="17sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/newCell2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text=""
                    android:textSize="17sp"
                    android:textStyle="bold" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/bleu_claire">

                <TextView
                    android:id="@+id/formations"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/bleu_claire"
                    android:clickable="true"
                    android:focusable="true"
                    android:fontFamily="@font/andika"
                    android:minHeight="48dp"
                    android:paddingStart="30dp"
                    android:paddingTop="0dp"
                    android:paddingEnd="30dp"
                    android:paddingBottom="0dp"
                    android:text="@string/formations_pratiques"
                    android:textColor="@color/BlueOf2cs"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/f1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text=""
                    android:textSize="17sp"
                    android:textStyle="bold" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/Orange_deteint">

                <TextView
                    android:id="@+id/coaching"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/Orange_deteint"
                    android:fontFamily="@font/andika"
                    android:minHeight="48dp"
                    android:paddingStart="30dp"
                    android:paddingTop="0dp"
                    android:paddingEnd="30dp"
                    android:paddingBottom="0dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:text="@string/coaching_m_moire"
                    android:textColor="@color/BlueOf2cs"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/f2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="" />

            </TableRow>


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/bleu_claire">

                <TextView
                    android:id="@+id/competences"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/bleu_claire"
                    android:fontFamily="@font/andika"
                    android:minHeight="48dp"
                    android:paddingStart="30dp"
                    android:paddingTop="0dp"
                    android:paddingEnd="30dp"
                    android:paddingBottom="0dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:text="@string/compl_ment_de_comp_tences"
                    android:textColor="@color/BlueOf2cs"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/f3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/Orange_deteint">

                <TextView
                    android:id="@+id/ipt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/Orange_deteint"
                    android:fontFamily="@font/andika"
                    android:minHeight="48dp"
                    android:paddingStart="30dp"
                    android:paddingTop="0dp"
                    android:paddingEnd="30dp"
                    android:paddingBottom="0dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:text="@string/informatique_pour_tous"
                    android:textColor="@color/BlueOf2cs"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/f4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/bleu_claire">

                <TextView
                    android:id="@+id/autres_pi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/bleu_claire"
                    android:clickable="true"
                    android:focusable="true"
                    android:fontFamily="@font/andika"
                    android:minHeight="48dp"
                    android:paddingStart="30dp"
                    android:paddingTop="0dp"
                    android:paddingEnd="30dp"
                    android:paddingBottom="0dp"
                    android:text="@string/autres_prestations_intellectuelles"
                    android:textColor="@color/BlueOf2cs"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/f5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="" />
            </TableRow>

        </TableLayout>

        <TextView
            android:id="@+id/pied_de_page"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-40dp"
            android:layout_marginBottom="-2dp"
            android:height="50dp"
            android:background="@drawable/bottom_rounded_corners"
            android:fontFamily="@font/noto_sans"
            android:gravity="center_horizontal"
            android:minHeight="48dp"
            android:paddingTop="7dp"
            android:text="@string/notre_expertise_votre_expansion_n_copyright_2cs_2024"
            android:textColor="@color/Orange_deteint"
            android:textSize="10sp"
            tools:ignore="SmallSp" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>